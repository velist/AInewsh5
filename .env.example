# 开发环境配置示例
# 复制此文件为 .env.local 并填入真实的API密钥

# GNews API配置
VITE_GNEWS_API_KEY=your_gnews_api_key_here

# SiliconFlow API配置 (用于翻译)
VITE_SILICONFLOW_API_KEY=your_siliconflow_api_key_here

# News API配置
VITE_NEWS_API_KEY=your_news_api_key_here

# NewsData API配置
VITE_NEWSDATA_API_KEY=your_newsdata_api_key_here

# 腾讯云翻译API配置
VITE_TENCENT_SECRET_ID=your_tencent_secret_id_here
VITE_TENCENT_SECRET_KEY=your_tencent_secret_key_here

# 百度翻译API配置
VITE_BAIDU_APP_ID=your_baidu_app_id_here
VITE_BAIDU_SECRET_KEY=your_baidu_secret_key_here

# 阿里云翻译API配置
VITE_ALIBABA_ACCESS_KEY_ID=your_alibaba_access_key_id_here
VITE_ALIBABA_ACCESS_KEY_SECRET=your_alibaba_access_key_secret_here